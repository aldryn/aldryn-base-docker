#!/bin/bash
/build/ngx_pagespeed.sh
/build/node.sh
/build/gulp.sh
/build/bower.sh
/build/wheels.sh
cp /build/addons-dev.pth /virtualenv/lib/python2.7/site-packages/aldryn-addons.pth
pipsi uninstall --yes pip-tools && pipsi install https://github.com/aldryncore/pip-tools/archive/1.1.6.1.tar.gz#egg=pip-tools==1.1.6.1
